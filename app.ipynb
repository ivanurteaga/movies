    "# Helpers, cleaner functions\n",
    "def find_2nd(string, substring):\n",
    "    return string.find(substring, string.find(substring) + 1)\n",
    "\n",
    "def find_1st(string, substring):\n",
    "    return string.find(substring, string.find(substring))"
